---
profile: min # null, min, basic, moderate,safety, shared, production
exclude_paths:
  - .git/
  - .github/
  - .vscode/
enable_list:
  - fqcn-builtins  # opt-in
warn_list:
  - line-length
  - yaml[line-length]
skip_list:
  - command-instead-of-shell
  - experimental
  - fqcn
  - name[missing]
  - no-changed-when
  - risky-file-permissions
  - role-name
  - schema[meta]
offline: true
